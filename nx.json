{
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "affected": {
    "defaultBase": "master"
  },
  "npmScope": "platform",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "common": {
      "tags": []
    },
    "components": {
      "tags": [],
      "implicitDependencies": ["common"]
    },
    "components-e2e": {
      "tags": [],
      "implicitDependencies": ["components"]
    },
    "demo": {
      "tags": []
    },
    "demo-e2e": {
      "tags": [],
      "implicitDependencies": ["demo"]
    },
    "editor": {
      "tags": []
    },
    "editor-e2e": {
      "tags": [],
      "implicitDependencies": ["editor"]
    },
    "elements": {
      "tags": []
    },
    "schematics": {
      "tags": []
    },
    "web-api": {
      "tags": []
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  }
}
