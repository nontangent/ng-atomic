{
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "affected": { "defaultBase": "master" },
  "npmScope": "platform",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "demo": { "tags": [] },
    "demo-e2e": { "tags": [], "implicitDependencies": ["demo"] },
    "common": { "tags": [] },
    "components": { "tags": [] },
    "schematics": { "tags": [] },
    "components-e2e": { "tags": [], "implicitDependencies": ["components"] },
    "elements": { "tags": []}
  }
}
